{
  "name": "contentkit",
  "version": "0.0.16",
  "description": "ContentKit is a powerful SDK for converting Markdown (or MDX) content into structured JSON data",
  "keywords": [
    "contentkit",
    "content",
    "sdk",
    "cli",
    "json",
    "markdown",
    "typescript"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "contentkit": "./distribution/cli.js"
  },
  "packageManager": "pnpm@10.8.0",
  "scripts": {
    "build": "tsc --skipLibCheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binary-blazer/contentkit.git"
  },
  "homepage": "https://github.com/binary-blazer/contentkit#readme",
  "bugs": {
    "url": "https://github.com/binary-blazer/contentkit/issues"
  },
  "files": [
    "assets",
    "distribution",
    "README.md",
    "LICENSE"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@ckjs/cli": "workspace:*",
    "@ckjs/types": "workspace:*",
    "@ckjs/next": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "typescript": "^5.8.3"
  },
  "exports": {
    "./types": "./distribution/types.js",
    "./types.d.ts": "./distribution/types.d.ts",
    "./next": "./distribution/next.js",
    "./next.d.ts": "./distribution/next.d.ts"
  }
}
